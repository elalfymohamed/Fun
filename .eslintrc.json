{
  "root":true,
  "parser": "babel-eslint",
    "env": {
        "browser": true,
        "es2021": true,
        "commonjs": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:import/errors",
      "plugin:import/warnings",
      "plugin:jsx-a11y/recommended",
      "plugin:react-hooks/recommended"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "settings": {
        "react": {
        "version": "detect"
        }
      },
    "plugins": [
    ],
    "rules": {
      "react/react-in-jsx-scope": "off"
    }
}
